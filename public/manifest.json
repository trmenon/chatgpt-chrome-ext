{
  "manifest_version": 3,
  "name": "You Query",
  "description": "Interact with ChatGPT with your queries",
  "version": "1.0.0",
  "action":{
    "default_popup": "index.html",
    "default_title": "Open",
    "default_icon": {
      "16": "icon-16.png",
      "32": "icon-32.png",
      "48": "icon-48.png",
      "128": "icon-128.png"
    }
  },
  "permission": [
    "scripting"
  ]
}
